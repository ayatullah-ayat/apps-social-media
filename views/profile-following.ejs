<%- include('includes/header-template') %>

<div class="container py-md-5 container--narrow">

  <%- include('includes/flash-message') %>
  <%- include('includes/sharedProfile') %>

  <div class="list-group">
    <% following.forEach(function(following) { %>
      <a href="/profile/<%= following.username %>" class="list-group-item list-group-item-action">
        <img class="avatar-tiny" src="<%= following.avatar %>">
        <%= following.username %>
      </a>
    <% }) %>
  </div>
</div>

<%- include('includes/footer-template') %>